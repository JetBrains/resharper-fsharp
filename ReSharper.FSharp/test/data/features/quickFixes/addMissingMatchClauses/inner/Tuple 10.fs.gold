module Say

match true, None, true with
| true, Some true -> ()
| true, None, true -> {selstart}failwith "todo"{selend}
| true, None, false -> failwith "todo"
| true, Some value, true -> failwith "todo"
| true, Some value, false -> failwith "todo"
| false, None, true -> failwith "todo"
| false, None, false -> failwith "todo"
| false, Some value, true -> failwith "todo"
| false, Some value, false -> failwith "todo"
