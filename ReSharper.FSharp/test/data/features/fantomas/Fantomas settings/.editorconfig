[*.fs]
fsharp_multi_line_lambda_closing_newline=true
fsharp_max_record_number_of_items=2
fsharp_max_array_or_list_number_of_items=2
fsharp_max_dot_get_expression_width=51
fsharp_disable_elmish_syntax=true
fsharp_keep_indent_in_branch=true
fsharp_blank_lines_around_nested_multiline_expressions=false
fsharp_bar_before_discriminated_union_declaration=true
fsharp_strict_mode=true
fsharp_record_multiline_formatter=number_of_items
fsharp_array_or_list_multiline_formatter=number_of_items
